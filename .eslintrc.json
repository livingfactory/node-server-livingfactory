{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off", 
        "no-trailing-spaces": "error",
        "semi": "error",
        "linebreak-style": ["error", "unix"]
    }
}
